<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <SiteUrlToLaunchAfterPublish>http://thirdeyedev.ca</SiteUrlToLaunchAfterPublish>
    <MSDeployServiceURL>clove.arvixe.com</MSDeployServiceURL>
    <DeployIisAppPath>thirdeyedev.ca</DeployIisAppPath>
    <RemoteSitePhysicalPath />
